}


html{overflow-y: scroll;}
html, body {height:100%;font-family: 'Nanum Gothic','나눔고딕','돋움','Dotum','맑은 고딕','Malgun Gothic',Sans-Serif; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,fieldset,p,span,button,th,td,a {border:0;margin:0;padding:0; font-weight:normal;}
h1,h2,h3,h4,h5,h6{letter-spacing:-1px;}
body {height:100%; font-size:15px; color:#222222; line-height:1.4;}
img{border:0;vertical-align:top;}
fieldset{margin:0;padding:0;border:0;}
textarea {border:1px solid #dddddd; padding:8px 0 9px 3px; margin:0; resize: none;}

ul,ol{list-style-type:none;}
dt,dd,h1,h2,h3,h4,h5,h6{margin:0;padding:0;}

select{height:35px; border:1px solid #dddddd; padding:2px 8px 3px 4px; vertical-align:middle; box-sizing: border-box;}
a,
a:link,
a:visited,
a:active{color:#2c2c2c;text-decoration:none;background-color:none;}
a:focus{color:#2c2c2c;text-decoration:none;font-weight:300;background-color:none;}
a:hover{text-decoration:underline;}
legend{visibility:hidden;overflow:hidden;top:-9999px;left:-9999px;width:0;height:0;font-size:0;line-height:0;}
hr{visibility:hidden;overflow:hidden;position:absolute;top:-9999px;left:-9999px;width:0;height:0;font-size:0;line-height:0;}
caption{overflow:hidden;visibility:hidden;width:0;height:0;font-size:0;line-height:0;}
em,address{font-style:normal;}
button{overflow:visible;padding:0;border:0;background:transparent;font-size:12px;  cursor:pointer; vertical-align: middle; margin:0 4px 0 8px;}
table{table-layout:fixed;border-collapse:collapse;}
input[type="text"] {border:1px solid #dddddd; height:35px; padding:2px 0 0 3px; font-size:13px; vertical-align: middle; margin:0; box-sizing: border-box;}
input[type="File"] {background:#ffffff; border:1px solid #dddddd; height:35px; padding:2px 0 0 3px; font-size:13px; vertical-align: middle; box-sizing: border-box;}
input[type="password"] {border:1px solid #dddddd; height:35px; padding:2px 0 0 3px; font-size:13px; box-sizing: border-box;}
input[type="checkbox"] {border-color: #dddddd; vertical-align:top; width:15px; height:15px;}
input[type="radio"] {border-color: #dddddd; margin:-1px 0 0 0; vertical-align:middle;}
strong {font-weight:700;}

/*a:focus{ outline: 3px dotted red}*/
.clfix:after {content: "."; display: block; height: 100%; clear: both; visibility: hidden;}
.clfix {display: inline-block;}
.clfixb {display: block;}
* html .clfix {height: 1%;} /* Hides from IE-mac */
.clfix {zoom:1;} /*for IE 5.5-7*/
* html #container{height:100%;} /* IE7 hack*/
* html #content {height:1%;}
.fl {float:left;}
.fr {float:right;}
.cl {clear:both;}
.rel {position:relative;}
.ac {text-align:center;}
.ar {text-align:right !important;}
.al {text-align:left;}
.at {vertical-align:top;}
.am {vertical-align:middle;}
.ab {vertical-align:bottom;}
.hand {cursor:pointer;}
.bgnone {background:none !important;}
.brnone {border:none !important;}
.brrnone {border-right:none;}
.bbline {background:#ffffff; border:1px solid #dddddd; margin:0; padding:0; width:100%; display:inline-block;}
.b {font-weight:700;}
.n {font-weight:normal;}
.u {text-decoration:underline;}
.lt1{ letter-spacing: -1px !important;}
.invisible{overflow:hidden; visibility:hidden; width:0; height:0; font-size:0; line-height:0;}
.hide{display: none !important;}
caption, legend {width:0; height:0; line-height:0; overflow:hidden; visibility:hidden; font-size:0;}

/* layout */
#wrap{ position:relative;width:100%;height:auto;background:#f3f3f3;}

/* ----- GNB ----- */
.headerWrapper {position:fixed; width:360px; height:100%; left:0; top:0; overflow:hidden; background:url('../images/common/bg.gif') repeat-y  0; z-index:100;}
.headerFixedBox {width:360px;}
#header .logoarea{height:105px; padding:50px 0 35px 30px; margin:0;}
#header #LnbArea .gnbDepth {position:relative; width:200px; padding:0 0 0 0; margin:0 0 0 0; z-index:100;}
#header #LnbArea .gnbDepth:after{content:'';display:block;clear:both;}
#header #LnbArea .gnbDepth li {padding:0; margin:0; float:left; display:block; }
#header #LnbArea .gnbDepth li:first-child a img {border-top:1px solid #dddddd; border-bottom:1px solid #dddddd;}
#header #LnbArea .gnbDepth li a {display:block; width:200px; text-align:center; margin:0;}
#header #LnbArea .gnbDepth li a img {box-sizing: border-box; border-bottom:1px solid #dddddd;}

#header #LnbArea .gnbDepth .depWrapper {width: 160px; position:absolute; left:195px; top:0px;  z-index:50; margin: 0 auto; box-sizing: border-box;font-size:15px;}
#header #LnbArea .gnbDepth .depWrapper ul {display:inline-block; float:left; width:166px; margin:0; padding:0 0 0 5px;}
#header #LnbArea .gnbDepth .depWrapper ul li {border-bottom:1px solid #797979;}
#header #LnbArea .gnbDepth .depWrapper ul li:first-child {border-top:1px solid #797979; border-bottom:1px solid #949391;}
#header #LnbArea .gnbDepth .depWrapper ul li a{display:block; text-align:left; padding:0 0px 0 10px;color:#ffffff;font-weight:400;line-height:49px;}
#header #LnbArea .gnbDepth .depWrapper ul li a:first-child{background:none;}
#header #LnbArea .gnbDepth .depWrapper ul li a:hover{color:#7fffee;text-decoration:underline;font-weight:400;}
#header #LnbArea .gnbDepth .depWrapper ul li .sel{color:#7fffee;text-decoration:underline;}
#header .gnbDepth2bg {position:fixed; width: 160px; height:100%; left:200px; top:0; overflow:hidden; z-index:10; padding:13px 0 20px 0; background-color:rgba(0,0,0,0.7);}

.loginbtn {display:inline-block; margin:0 0 35px 24px;}
.loginbtn li {float:left; margin:0 3px;}
.logout {width:148px; text-align:center;}

.Micon {display:inline-block; margin:25px 0 40px 10px;}
.Micon li {float:left; margin:0 9px;}

.cscenter {display:inline-block; width:200px; text-align:center; margin:0 0 35px 0;}

#wrap #container{width:100%; height:100%; box-sizing:border-box;}
#wrap #container #contents {position:relative; width:1000px; height:100%; min-height:800px; margin-left:200px; padding:55px 30px 100px 30px; background:#ffffff; box-sizing:border-box; z-index:99;}
#wrap #container #contents img {max-width:940px;}
#wrap.bg01 {background:#e2e2e2 url('../images/sub/bg_body01.png') no-repeat 1200px 0;}
