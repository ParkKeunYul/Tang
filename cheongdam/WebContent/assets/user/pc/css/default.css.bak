/* 블릿형 list */
.listLDotted {padding:10px 0 0 15px;}
.listLDotted>li {position:relative; padding:0 0 0 10px; margin-bottom:5px; line-height:1.5;}
.listLDotted>li:before {content:'·'; position:absolute; top:0; left:0; width:3px; font-weight:bold;}

.listLstar {padding:5px 0 10px 0;}
.listLstar>li {position:relative; padding:0 0 0 15px; margin-bottom:5px; line-height:1.5;}
.listLstar>li:before {content:'*'; position:absolute; top:0; left:0; width:3px; font-weight:bold;}

