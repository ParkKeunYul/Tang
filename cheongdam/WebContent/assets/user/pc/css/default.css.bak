#wrap #footer{width: 100%; position:relative; padding:0 0 60px 200px; background:#444444; overflow:hidden; box-sizing:border-box;}
#wrap #footer #footer_inner {width: 100%; min-width:1000px; margin: 0 auto;}
#wrap #footer .copyright {display:block; font-size:13px; padding:10px 0 0 30px; color:#d5d5d5;}
#wrap #footer .copyright li {padding:2px 0;;}
#wrap #footer .copyright li p {background:url('../images/sub/list_s02.png') no-repeat left 6px; display:inline-block; padding:0 13px 0 17px; }
.pnone {background:none !important; padding-left:0 !important;}
